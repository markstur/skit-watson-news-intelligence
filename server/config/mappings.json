      "cloudfoundry:$.discovery[0].credentials.url",
      "env:service_watson_discovery:$.url",
      "file:/server/localdev-config.json:$.watson_discovery_url"
    ]
  },
  "watson_discovery_username": {
    "searchPatterns": [
      "cloudfoundry:$.discovery[0].credentials.username",
